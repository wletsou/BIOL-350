	doi = {10.1093/bioinformatics/btr341},
	abstract = {Motivation: Performing experiments with simulated data is an inexpensive approach to evaluating competing experimental designs and analysis methods in genome-wide association studies. Simulation based on resampling known haplotypes is fast and efﬁcient and can produce samples with patterns of linkage disequilibrium (LD), which mimic those in real data. However, the inability of current methods to simulate multiple nearby disease SNPs on the same chromosome can limit their application.},
	language = {en},
	number = {16},
	urldate = {2024-03-29},
	journal = {Bioinformatics},
	author = {Su, Zhan and Marchini, Jonathan and Donnelly, Peter},
	month = aug,
	year = {2011},
	pages = {2304--2305},
	file = {HAPGEN2- simulation of multiple disease SNPs.pdf:/Users/wletsou/Library/CloudStorage/OneDrive-NewYorkInstituteofTechnology/Courses/BIOL 350 Spring 2024/HAPGEN2- simulation of multiple disease SNPs.pdf:application/pdf},
}

