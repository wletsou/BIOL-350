
@article{manichaikul_robust_2010,
	title = {Robust relationship inference in genome-wide association studies},
	volume = {26},
	issn = {1367-4811, 1367-4803},
	url = {https://academic.oup.com/bioinformatics/article/26/22/2867/228512},
	doi = {10.1093/bioinformatics/btq559},
	abstract = {Motivation: Genome-wide association studies (GWASs) have been widely used to map loci contributing to variation in complex traits and risk of diseases in humans. Accurate speciﬁcation of familial relationships is crucial for family-based GWAS, as well as in population-based GWAS with unknown (or unrecognized) family structure. The family structure in a GWAS should be routinely investigated using the SNP data prior to the analysis of population structure or phenotype. Existing algorithms for relationship inference have a major weakness of estimating allele frequencies at each SNP from the entire sample, under a strong assumption of homogeneous population structure. This assumption is often untenable.},
	language = {en},
	number = {22},
	urldate = {2024-03-21},
	journal = {Bioinformatics},
	author = {Manichaikul, Ani and Mychaleckyj, Josyf C. and Rich, Stephen S. and Daly, Kathy and Sale, Michèle and Chen, Wei-Min},
	month = nov,
	year = {2010},
	pages = {2867--2873},
	file = {Robust relationship inference in genome-wide association studies.pdf:/Users/wletsou/Library/CloudStorage/OneDrive-NewYorkInstituteofTechnology/Courses/BIOL 350 Spring 2023/Robust relationship inference in genome-wide association studies.pdf:application/pdf},
}

@article{changalidis_biogwas_2023,
	title = {{bioGWAS}: {A} {Simple} and {Flexible} {Tool} for {Simulating} {GWAS} {Datasets}},
	volume = {13},
	copyright = {https://creativecommons.org/licenses/by/4.0/},
	issn = {2079-7737},
	shorttitle = {{bioGWAS}},
	url = {https://www.mdpi.com/2079-7737/13/1/10},
	doi = {10.3390/biology13010010},
	abstract = {Genome-wide association studies (GWAS) have proven to be a powerful tool for the identification of genetic susceptibility loci affecting human complex traits. In addition to pinpointing individual genes involved in a particular trait, GWAS results can be used to discover relevant biological processes for these traits. The development of new tools for extracting such information from GWAS results requires large-scale datasets with known biological ground truth. Simulation of GWAS results is a powerful method that may provide such datasets and facilitate the development of new methods. In this work, we developed bioGWAS, a simple and flexible pipeline for the simulation of genotypes, phenotypes, and GWAS summary statistics. Unlike existing methods, bioGWAS can be used to generate GWAS results for simulated quantitative and binary traits with a predefined set of causal genetic variants and/or molecular pathways. We demonstrate that the proposed method can recapitulate complete GWAS datasets using a set of reported genome-wide associations. We also used our method to benchmark several tools for gene set enrichment analysis for GWAS data. Taken together, our results suggest that bioGWAS provides an important set of functionalities that would aid the development of new methods for downstream processing of GWAS results.},
	language = {en},
	number = {1},
	urldate = {2024-03-29},
	journal = {Biology},
	author = {Changalidis, Anton I. and Alexeev, Dmitry A. and Nasykhova, Yulia A. and Glotov, Andrey S. and Barbitoff, Yury A.},
	month = dec,
	year = {2023},
	pages = {10},
	file = {bioGWAS- A Simple and Flexible Tool for Simulating GWAS Datasets.pdf:/Users/wletsou/Library/CloudStorage/OneDrive-NewYorkInstituteofTechnology/Courses/BIOL 350 Spring 2024/bioGWAS- A Simple and Flexible Tool for Simulating GWAS Datasets.pdf:application/pdf},
}

